{
  "name": "free-api-hub",
  "version": "2.1.0",
  "description": "Production-ready API Gateway with WebSocket, PostgreSQL, Analytics & AI",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "test": "jest",
    "db:setup": "psql -U postgres -f src/db/schema.sql",
    "db:migrate": "node src/db/migrate.js",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:build": "docker-compose build"
  },
  "keywords": [
    "api-gateway",
    "apisix",
    "dragonfly",
    "redis",
    "websocket",
    "postgresql",
    "swagger",
    "openapi",
    "real-time",
    "analytics",
    "free",
    "open-source"
  ],
  "author": "George Pricop <george@free-api-hub.dev>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Gzeu/free-api-hub.git"
  },
  "dependencies": {
    "express": "^4.18.2",
    "ws": "^8.16.0",
    "pg": "^8.11.3",
    "redis": "^4.6.12",
    "axios": "^1.6.5",
    "yaml": "^2.3.4",
    "@google/generative-ai": "^0.1.3",
    "winston": "^3.11.0",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "express-rate-limit": "^7.1.5",
    "dotenv": "^16.3.1",
    "swagger-ui-express": "^5.0.0",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "uuid": "^9.0.1",
    "joi": "^17.12.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.4"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  }
}
